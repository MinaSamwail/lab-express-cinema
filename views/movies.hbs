<div class="allCard">
    {{#each allMovies}}
    <div class="card">
        <img src="{{this.image}}" alt="{{this.name}}">
        <div class="card-description">
            <p>{{this.title}}</p>
            <a href="/movieDescription/{{this._id}}">See more</a>
        </div>
    </div>
    {{/each}}
</div>